queueonteleport("loadstring(game:HttpGet('https://raw.githubusercontent.com/LuaSecurity/Sneak-Thief/refs/heads/main/AutoFarm'))()")

repeat task.wait() until game:IsLoaded()
repeat task.wait() until game.Players and game.Players.LocalPlayer and game.Players.LocalPlayer.Character
repeat task.wait() until game.ReplicatedStorage

local function waitForDescendants(instance, names)
    for _, name in ipairs(names) do
        repeat task.wait() until instance:FindFirstChild(name, true)
    end
end

if game.PlaceId == 492229395 then
    waitForDescendants(game.ReplicatedStorage, {"Events", "Events.TeleportPlayer"})
    game.ReplicatedStorage.Events.TeleportPlayer:FireServer(140273672157780)
elseif game.PlaceId == 140273672157780 then
    waitForDescendants(game.ReplicatedStorage, {"GameEvents", "GameEvents.InteractionEvents", "GameEvents.InteractionEvents.Steal"})
    waitForDescendants(workspace, {"Stealables", "EndBox"})

    local stealEvent = game.ReplicatedStorage.GameEvents.InteractionEvents.Steal
    local hrp = game.Players.LocalPlayer.Character:WaitForChild("HumanoidRootPart")

    for _, item in ipairs(workspace.Stealables:GetDescendants()) do
        task.spawn(function()
            stealEvent:InvokeServer(item)
        end)
    end

    while task.wait(0.05) do
        firetouchinterest(hrp, workspace.EndBox, 0)
        firetouchinterest(hrp, workspace.EndBox, 1)
    end
end
